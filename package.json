{
  "name": "openapi-fetch-gen",
  "version": "0.2.5",
  "description": "OpenAPI TypeScript client generator with native fetch support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "openapi-fetch-gen": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/cli.ts",
    "test": "jest --passWithNoTests",
    "lint": "eslint .",
    "lint:fix": "eslint src/**/*.{js,ts} --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "gen": "node dist/cli.js --base-url http://localhost:8000/api/v1/schema/ -o generated --hooks react"
  },
  "keywords": [
    "openapi",
    "typescript",
    "fetch",
    "code-generator",
    "api-client"
  ],
  "author": "Santiago A. Pereira",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/santiagoapp/openapi-fetch-gen.git"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.7.9",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "commander": "^12.1.0",
    "eslint": "^9.13.0",
    "jest": "^29.7.0",
    "openapi-types": "^12.1.3",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "js-yaml": "^4.1.0"
  }
}
